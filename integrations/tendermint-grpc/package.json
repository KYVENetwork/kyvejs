{
  "name": "@kyvejs/tendermint-grpc",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:binaries": "yarn build && rimraf out && pkg --no-bytecode --public-packages '*' --output out/kyve package.json",
    "start": "node ./src/runtime.js",
    "format": "prettier --write ."
  },
  "bin": "./src/runtime.js",
  "pkg": {
    "scripts": "./src/runtime.js",
    "targets": [
      "latest-linux-x64",
      "latest-linux-arm64",
      "latest-macos-x64"
    ],
    "outputPath": "out"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.2",
    "ajv": "^8.12.0",
    "axios": "^0.27.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "pkg": "^5.8.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}