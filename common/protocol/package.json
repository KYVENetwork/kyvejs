{
  "name": "@kyvejs/protocol",
  "version": "1.1.4",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KYVENetwork/kyvejs.git"
  },
  "author": "Troy Kessler <troy@kyve.network>, John Letey <john@kyve.network>, ruslan glaznyov <ruslan.glaznyov@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": "./dist/src/index.js",
  "pkg": {
    "targets": [
      "latest-linux-x64"
    ],
    "outputPath": "out",
    "noBytecode": true,
    "publicPackages": [
      "*"
    ]
  },
  "scripts": {
    "build": "rimraf dist && yarn format && tsc",
    "build:binaries": "yarn build && rimraf out && pkg package.json",
    "build:docker": "docker build -t kyvejs/protocol .",
    "test": "jest",
    "format": "prettier --write . --ignore-path ./.prettierignore",
    "doc": "typedoc"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.370.0",
