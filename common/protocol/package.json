    "@grpc/grpc-js": "^1.9.2",
    "@kyvejs/sdk": "1.1.1",
    "@kyvejs/types": "1.2.0",
    "@types/cli-progress": "^3.9.2",
    "@types/jsonfile": "^6.0.1",
    "arweave": "^1.10.17",
    "axios": "^0.27.2",
    "bignumber.js": "^9.1.2",
    "clone": "^2.1.2",
    "commander": "^9.4.1",
    "diff": "^5.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^10.0.1",
    "jsonfile": "^6.1.0",
    "jszip": "^3.10.1",
    "level": "^8.0.0",
    "prando": "^6.0.1",
    "prom-client": "^14.1.0",
    "protobufjs": "^6.11.4",
    "seedrandom": "^3.0.5",
    "semver": "^7.5.3",
    "tslog": "^3.2.2",
    "unique-names-generator": "^4.6.0",
    "uuid": "^9.0.0"
